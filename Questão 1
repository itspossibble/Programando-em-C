#include <stdio.h>                 //Observe o trecho de código abaixo: int INDICE = 13, SOMA = 0, K = 0;
                                   //Enquanto K < INDICE faça { K = K + 1; SOMA = SOMA + K; }
                                   //Imprimir(SOMA);
                                   //o final do processamento, qual será o valor da variável SOMA?

int main() {
    int INDICE = 13;
    int SOMA = 0;
    int K = 0;

    while (K < INDICE) {
        K = K + 1;
        if (K != INDICE) {
            SOMA = SOMA + K;
        }
    }

    printf("%d\n", SOMA);
    return 0;
}

